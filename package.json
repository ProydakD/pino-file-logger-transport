{
  "name": "pino-file-logger-transport",
  "version": "1.4.2",
  "description": "A Pino transport for file logging with rotation and archiving capabilities",
  "keywords": [
    "pino",
    "logger",
    "transport",
    "file",
    "rotation",
    "archive"
  ],
  "author": "Dmitriy Proydak <proydak@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProydakD/pino-file-logger-transport.git"
  },
  "bugs": {
    "url": "https://github.com/ProydakD/pino-file-logger-transport/issues"
  },
  "homepage": "https://github.com/ProydakD/pino-file-logger-transport#readme",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "format": "prettier --write .",
    "build": "tsc",
    "dev": "vitest watch",
    "prepare": "npm run build",
    "prerelease": "npm run lint && npm test && npm run build",
    "release": "standard-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:beta": "standard-version --prerelease beta",
    "postrelease": "git push --follow-tags"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "pino-abstract-transport": "^2.0.0",
    "sonic-boom": "^4.2.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/node": "^24.5.2",
    "@types/pino": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.6.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "pino": "^9.0.0"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
